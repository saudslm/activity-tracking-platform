####### FILE: .env.example

# Database
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/timetrack

# Redis
REDIS_URL=redis://localhost:6379
REDIS_TLS_URL=rediss://localhost:6379

# Cloudflare R2
R2_ACCOUNT_ID=your_cloudflare_account_id
R2_ACCESS_KEY_ID=your_r2_access_key
R2_SECRET_ACCESS_KEY=your_r2_secret
R2_BUCKET_ORIGINAL=screenshots-original
R2_BUCKET_BLURRED=screenshots-blurred
R2_PUBLIC_URL=https://pub-xxxxx.r2.dev

# ClickUp
CLICKUP_CLIENT_ID=your_clickup_client_id
CLICKUP_CLIENT_SECRET=your_clickup_secret
CLICKUP_REDIRECT_URI=http://localhost:5173/api/clickup/callback

# Auth
SESSION_SECRET=generate_with_openssl_rand_hex_32
JWT_SECRET=generate_with_openssl_rand_hex_32

# App
APP_URL=http://localhost:5173
NODE_ENV=development